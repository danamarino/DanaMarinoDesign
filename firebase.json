        ],
        "predeploy": [
            "npm run clean",
            "npm run build"
        ]
    },
    "database": {
        "rules": "database.rules.json"
    },
    "functions": {
        "predeploy": [
            "npm --prefix \"$RESOURCE_DIR\" run lint"
        ]
    }
