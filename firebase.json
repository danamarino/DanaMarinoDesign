            },
            {
                "source": "/weddings_heather_james.html",
                "destination": "/weddings/heather_james_sanibel",
                "type": 301
            },
            {
                "source": "/weddings_courtney_jim.html",
                "destination": "/weddings/courtney_jim_estero",
                "type": 301
            },
            {
